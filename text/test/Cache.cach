test/MA-S-IV-Guidence-1_page-0001.jpg	[{"transcription": "Psychological Test", "points": [[521, 154], [719, 154], [719, 177], [521, 177]], "difficult": false}, {"transcription": "Definition: - A Psychological test is a standardized measure of a sample of a person's", "points": [[148, 226], [1091, 226], [1091, 250], [148, 250]], "difficult": false}, {"transcription": "behaviour that is used to measure the individual differences that exist among people. A", "points": [[147, 251], [1093, 252], [1093, 279], [147, 278]], "difficult": false}, {"transcription": "psychological test is an objective and standardized measure of an individual's mental and/or", "points": [[146, 283], [1092, 280], [1092, 307], [146, 310]], "difficult": false}, {"transcription": "behavioural characteristics. A Psychological test is a systematic procedure or observing", "points": [[147, 309], [1092, 312], [1092, 339], [147, 336]], "difficult": false}, {"transcription": "person's behaviour or performing, describing it with the aid of a numerical scale or category", "points": [[146, 339], [1095, 339], [1095, 369], [146, 369]], "difficult": false}, {"transcription": "system. Mostly tests are used as a way of measuring differences between people or", "points": [[148, 370], [1093, 369], [1093, 396], [148, 397]], "difficult": false}, {"transcription": "differences in the same person over time. Psychological tests are written, visual, or verbal", "points": [[148, 396], [1093, 396], [1093, 426], [148, 426]], "difficult": false}, {"transcription": "evaluations administered to assess the cognitive and emotional functioning of children and", "points": [[149, 427], [1091, 427], [1091, 451], [149, 451]], "difficult": false}, {"transcription": "adults.", "points": [[147, 455], [217, 455], [217, 480], [147, 480]], "difficult": false}, {"transcription": "Need for psychological test: -", "points": [[149, 514], [458, 514], [458, 537], [149, 537]], "difficult": false}, {"transcription": "1.", "points": [[148, 557], [171, 557], [171, 583], [148, 583]], "difficult": false}, {"transcription": "Identifies weaknesses and strength", "points": [[222, 555], [571, 556], [571, 583], [222, 582]], "difficult": false}, {"transcription": "2.", "points": [[146, 599], [172, 599], [172, 631], [146, 631]], "difficult": false}, {"transcription": "Supports individualized lesson plans", "points": [[223, 601], [592, 600], [592, 627], [223, 628]], "difficult": false}, {"transcription": "3.", "points": [[146, 644], [172, 644], [172, 677], [146, 677]], "difficult": false}, {"transcription": "Enables placement decisions", "points": [[222, 645], [513, 646], [513, 673], [222, 672]], "difficult": false}, {"transcription": "4.", "points": [[146, 689], [172, 689], [172, 720], [146, 720]], "difficult": false}, {"transcription": "Monitors progress", "points": [[221, 690], [409, 692], [408, 719], [221, 717]], "difficult": false}, {"transcription": "5.", "points": [[146, 733], [172, 733], [172, 764], [146, 764]], "difficult": false}, {"transcription": "Identifying disabilities", "points": [[224, 736], [450, 736], [450, 760], [224, 760]], "difficult": false}, {"transcription": "6.", "points": [[146, 778], [172, 778], [172, 806], [146, 806]], "difficult": false}, {"transcription": "Helping the administrative and academic works", "points": [[224, 780], [700, 780], [700, 804], [224, 804]], "difficult": false}, {"transcription": "7.", "points": [[146, 821], [172, 821], [172, 850], [146, 850]], "difficult": false}, {"transcription": "Vocational ability.", "points": [[222, 821], [411, 824], [410, 851], [222, 847]], "difficult": false}, {"transcription": "For children, academic achievement, ability psychological tests may be used as tools in", "points": [[147, 868], [1092, 868], [1092, 894], [147, 894]], "difficult": false}, {"transcription": "school placement, in determining the presence of a learning disability or a developmental", "points": [[148, 896], [1091, 896], [1091, 923], [148, 923]], "difficult": false}, {"transcription": "delay in identifying giftedness, or in tracking intellectual development. It may also be used", "points": [[149, 925], [1093, 925], [1093, 952], [149, 952]], "difficult": false}, {"transcription": "with teens and young adults to determine vocational ability (e.g., in career counselling). Tests", "points": [[148, 953], [1092, 954], [1092, 981], [148, 980]], "difficult": false}, {"transcription": "are administered for a wide variety of reasons, from diagnosing psychopathology (e.g.,", "points": [[146, 980], [1093, 983], [1093, 1013], [146, 1010]], "difficult": false}, {"transcription": "personality disorder, depressive disorder) to screening job candidates. They may be used in", "points": [[145, 1011], [1093, 1009], [1093, 1039], [145, 1041]], "difficult": false}, {"transcription": "an educational setting to determine personality strengths and weaknesses.", "points": [[149, 1041], [885, 1042], [885, 1066], [149, 1065]], "difficult": false}, {"transcription": "Major Uses of Tests: -", "points": [[148, 1117], [386, 1119], [386, 1144], [148, 1142]], "difficult": false}, {"transcription": "The basic use of tests is to provide information for decision makers.", "points": [[149, 1148], [828, 1148], [828, 1172], [149, 1172]], "difficult": false}, {"transcription": "• Selection or placement", "points": [[147, 1178], [394, 1180], [394, 1205], [147, 1203]], "difficult": false}, {"transcription": "• Diagnosis", "points": [[145, 1207], [266, 1211], [265, 1238], [144, 1234]], "difficult": false}, {"transcription": "• Accountability evaluations", "points": [[146, 1238], [434, 1242], [433, 1268], [146, 1265]], "difficult": false}, {"transcription": "• Judging progress and following trends", "points": [[145, 1268], [550, 1269], [550, 1299], [145, 1298]], "difficult": false}, {"transcription": "• Self- discovery", "points": [[146, 1298], [323, 1304], [323, 1331], [145, 1326]], "difficult": false}, {"transcription": "Main Characteristics of a Good Psychological Test", "p